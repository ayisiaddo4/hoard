{
  "name": "mobile",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "babel-eslint": "8.2.1",
    "babel-preset-react-native-stage-0": "^1.0.1",
    "eslint": "^4.16.0",
    "eslint-plugin-react": "7.5.1",
    "eslint-plugin-react-native": "^3.2.1",
    "jest": "^22.2.1",
    "prettier": "1.10.2",
    "react-test-renderer": "16.0.0-alpha.12"
  },
  "scripts": {
    "start": "react-native start",
    "android": "ENVFILE=.env.staging react-native run-android",
    "ios": "ENVFILE=.env.staging react-native run-ios --scheme 'hoardinvest - Staging'",
    "test": "node node_modules/jest/bin/jest.js --watch",
    "lint": "yarn eslint src --ext js",
    "format": "prettier ./src/**/*.js --write"
  },
  "jest": {
    "preset": "react-native",
    "modulePaths": [
      "<rootDir>/src"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!react-native|react-navigation)/"
    ],
    "moduleFileExtensions": [
      "js"
    ],
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.js$",
    "testPathIgnorePatterns": [
      "\\.snap$",
      "<rootDir>/node_modules/"
    ],
    "cacheDirectory": ".jest/cache"
  },
  "rnpm": {
    "assets": [
      "./src/assets/fonts/"
    ]
  },
  "dependencies": {
    "ethers": "^2.2.4",
    "lottie-react-native": "2.2.7",
    "prop-types": "^15.5.10",
    "react": "16.0.0-alpha.12",
    "react-native": "^0.47.0",
    "react-native-animatable": "^1.2.4",
    "react-native-config": "^0.11.5",
    "react-native-hockeyapp": "^0.5.1",
    "react-native-jumio-mobilesdk": "https://github.com/Jumio/mobile-react.git#v2.10.0",
    "react-native-splash-screen": "3.0.6",
    "react-native-svg": "^5.4.2",
    "react-native-vector-icons": "^4.5.0",
    "react-navigation": "^1.5.9",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "redux-saga": "0.16.0"
  }
}
